<head>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>

<h1>User List</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @paginated_users.each do |user| %>
      <tr>
        <td><%= user['name'] %></td>
        <td><%= user['email'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @paginated_users %>
